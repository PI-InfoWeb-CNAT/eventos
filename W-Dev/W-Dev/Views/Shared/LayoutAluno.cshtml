<!DOCTYPE html>
@using W_Dev.Infraestrutura;
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Aluno</title>
    <link rel="stylesheet" type="text/css" href="~/css/geralAluno.css" />
